{
  "name": "geekymenu",
  "version": "1.0.0",
  "description": "A terminal-based application launcher for Linux",
  "main": "geekymenu.js",
  "bin": {
    "geekymenu": "./geekymenu.js"
  },
  "scripts": {
    "start": "node geekymenu.js",
    "build": "pkg .",
    "build-linux": "pkg . --targets node18-linuxstatic-x64",
    "build-all": "pkg . --targets node18-linuxstatic-x64,node18-macos-x64,node18-win-x64"
  },
  "pkg": {
    "assets": [
      "node_modules/blessed/usr/**/*"
    ],
    "targets": ["node18-linuxstatic-x64"],
    "outputPath": "dist"
  },
  "dependencies": {
    "blessed": "^0.1.81",
    "glob": "^11.0.3"
  },
  "devDependencies": {
    "pkg": "^5.8.1"
  },
  "keywords": ["launcher", "terminal", "linux", "desktop", "menu"],
  "author": "",
  "license": "MIT"
}
